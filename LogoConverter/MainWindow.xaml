<Window x:Class="LogoConverter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LogoConverter"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="button" Content="LoadAndConvert" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="202" Click="button_Click"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="274" Margin="10,35,0,0" TextWrapping="Wrap" AcceptsReturn="True" VerticalAlignment="Top" Width="497"/>

    </Grid>
</Window>
